{
    "url": "https://fuelprices.esso.co.uk/latestdata.json",
    "dateFormat": "DD/MM/YYYY HH:mm:ss",
    "template": {
        "{{#each stations}}": {
            "site_id": "{{site_id}}",
            "company": "Esso",
            "e5": "{{#? prices.E5 * 100}}",
            "e10": "{{#? prices.E10 * 100}}",
            "b7": "{{#? prices.B7 * 100}}",
            "sdv": "{{#? prices.SDV * 100}}",
            "address": "{{address}}",
            "postcode": "{{postcode}}",
            "location": {
                "type": "Point",
                "coordinates": [
                    "{{location.longitude}}",
                    "{{location.latitude}}"
                ] 
            },
            "created_at": "{{$root.last_updated}}"
        }
    }
} 